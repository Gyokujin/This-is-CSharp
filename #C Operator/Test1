using System;

namespace Test1
{
    internal class Program
    {
        // 다음 코드에서 a와 b는 각각 얼마일까요?

        static void Main(string[] args)
        {
            int a = 8 >> 1; // 1000 => 0100 이므로 4
            int b = a >> 2; // 0100 => 0001 이므로 1
            Console.WriteLine(a);
            Console.WriteLine(b);
        }
    }
}

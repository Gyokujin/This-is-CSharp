using System;

namespace Test1
{
    internal class Program
    {
        // 다음과 같은 결과를 출력하는 프로그램을 for 문을 이용하여 작성하세요.
        // *
        // **
        // ***
        // ****
        // *****

        static void Main(string[] args)
        {
            string message = string.Empty;

            for (int i = 0; i < 5; i++)
            {
                message += "*";
                Console.WriteLine(message);
            }
        }
    }
}

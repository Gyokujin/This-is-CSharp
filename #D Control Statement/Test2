using System;

namespace Test2
{
    internal class Program
    {
        // 다음과 같은 결과를 출력하는 프로그램을 for 문을 이용하여 작성하세요.
        // *****
        // ****
        // ***
        // **
        // *

        static void Main(string[] args)
        {
            for (int i = 5; i > 0; i--)
            {
                for (int j = 0; j < i; j++)
                {
                    Console.Write("*");
                }

                Console.WriteLine();
            }
        }
    }
}

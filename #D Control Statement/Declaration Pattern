using System;

namespace Declaration_Pattern
{
    internal class Program
    {
        static void Main(string[] args)
        {
            object foo = 23;
            if (foo is int bar) // foo가 int인 경우 foo를 int 형식으로 변환한 값을 bar에 할당한다.
            {
                Console.WriteLine(bar);
            }
        }
    }
}

using System;

namespace IntegralConversion
{
    internal class Program
    {
        static void Main(string[] args)
        {
            sbyte a = 127;
            Console.WriteLine(a);

            int b = (int)a;
            Console.WriteLine(b);

            int x = 128; // sbyte의 최대값 127보다 1 큰 수
            Console.WriteLine(x);

            sbyte y = (sbyte)x; // sbyte의 범위는 -128 ~ 127이기 때문에 128의 값을 대입하면 오버 플로가 발생한다.
            Console.WriteLine(y);
        }
    }
}

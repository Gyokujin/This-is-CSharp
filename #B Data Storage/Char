using System;

namespace Char
{
    internal class Program
    {
        static void Main(string[] args)
        {
            char a = '안';// 문자는 작은따옴표로 표시한다.
            char b = '녕';
            char c = '하';
            char d = '세';
            char e = '요';

            Console.Write(a); // Console.Write() 메소드는 데이터를 출력한 후 줄을 바꾸지 않는다.
            Console.Write(b);
            Console.Write(c);
            Console.Write(d);
            Console.Write(e);
            Console.WriteLine(); // Console.WriteLine() 메소드는 데이터를 출력한 후 줄을 바꾼다.
        }
    }
}
